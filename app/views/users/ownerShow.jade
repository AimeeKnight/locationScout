extends ../shared/template

block content
  input#ownerId(type='hidden', value= owner._id)
  .row
    .small-12.columns
      div.panel
        //img(src= owner.userPhoto).user-img
        h1= owner.name
        h4= owner.email
  .row
    .small-12.columns
      each listing in listings
        input.listingId(type='hidden', value= listing._id)
        ul
          li= listing.name
          li= listing.amount
          li= listing.address
          li= listing.description
        each reservation in listing.reservations
          ul
            li= reservation.artistName
            //li = reservation.reservedDate.toDateString();

